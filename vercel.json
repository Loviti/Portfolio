{
  "functions": {
    "app/api/chat/route.ts": {
      "runtime": "edge"
    },
    "app/api/contact/route.ts": {
      "runtime": "edge"
    },
    "app/api/admin/upload/route.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ]
} 